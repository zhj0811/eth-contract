    constructor(
        string memory name,
        string memory symbol,
        string memory baseURI
    ) ERC721(name, symbol) {
        m_BaseURI = baseURI;
        deployTime = block.timestamp;
        // Horse memory _hor;
        // _safeMint(msg.sender, 0);

